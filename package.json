{
  "name": "google-marketing-hub-mcp",
  "version": "0.1.0",
  "description": "Remote MCP Server for Google Marketing APIs on Cloudflare Workers",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "d1:migrate": "wrangler d1 execute ghub-tokens --file=./migrations/0001_initial.sql",
    "d1:migrate:local": "wrangler d1 execute ghub-tokens --local --file=./migrations/0001_initial.sql",
    "logs": "wrangler tail --format=pretty",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.7.2",
    "wrangler": "^3.93.0"
  }
}
